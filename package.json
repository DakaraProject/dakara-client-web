{
  "name": "dakara-client-web",
  "version": "1.8.0-dev",
  "description": "Web client for Dakara Project",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DakaraProject/dakara-client-web.git"
  },
  "author": "Nadeflore, Neraste",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DakaraProject/dakara-client-web/issues"
  },
  "projectHomepage": "https://github.com/DakaraProject/dakara-client-web",
  "private": true,
  "proxy": "http://localhost:8000",
  "dependencies": {
    "classnames": "^2.3.1",
    "dayjs": "^1.11.0",
    "normalize.css": "^8.0.1",
    "postcss": "^8.4.12",
    "query-string": "^7.1.1",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-highlight-words": "^0.17.0",
    "react-redux": "^7.2.7",
    "react-router-dom": "^6.3.0",
    "react-scripts": "^5.0.0",
    "react-transition-group": "^4.4.2",
    "redux": "^4.1.2",
    "redux-localstorage": "^0.4.1",
    "redux-thunk": "^2.4.1",
    "sass": "^1.49.10"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app",
    "plugins": [
      "simple-import-sort",
      "import"
    ],
    "rules": {
      "simple-import-sort/imports": [
        "error",
        {
          "groups": [
            [
              "^\\u0000"
            ],
            [
              "^@?\\w"
            ],
            [
              "^(actions|components|eventManagers|middleware|reducers|serverPropTypes|style|utils)"
            ],
            [
              "^"
            ],
            [
              "^\\."
            ]
          ]
        }
      ],
      "import/first": "error",
      "import/newline-after-import": "error",
      "import/no-duplicates": "error",
      "max-len": [
        "error",
        {
          "code": 88
        }
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "customize-cra": "^1.0.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "react-app-rewired": "^2.2.1",
    "stylelint": "^14.6.1",
    "stylelint-config-sass-guidelines": "^9.0.1",
    "stylelint-webpack-plugin": "^3.2.0"
  }
}
